# Database Configuration
DATABASE_URL=postgresql://postgres:YOUR_PASSWORD_HERE@localhost:5432/document_management_system

# JWT Secret (Generate a strong random secret!)
# You can generate one with: node -e "console.log(require('crypto').randomBytes(64).toString('hex'))"
JWT_SECRET=CHANGE_THIS_TO_A_LONG_RANDOM_SECRET_MINIMUM_32_CHARACTERS

# Server Configuration
PORT=3001
UPLOAD_PATH=./uploads
NODE_ENV=development

# Gemini AI API Key (Get from: https://makersuite.google.com/app/apikey)
GEMINI_API_KEY=YOUR_GEMINI_API_KEY_HERE
